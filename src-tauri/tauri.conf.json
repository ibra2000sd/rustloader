{
  "productName": "Rustloader",
  "version": "1.0.0",
  "identifier": "com.rustloader.dev",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Rustloader",
        "width": 900,
        "height": 700,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "shell": {
      "open": true
    },
    "fs": {
      "scope": ["$HOME/Downloads"]
    },
    "dialog": {
      "all": true
    },
    "path": {
      "all": true
    }
  }
}